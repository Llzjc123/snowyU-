/* pages/distributePage/distributeOrders/index.wxss */
.distribute-bill-container {
  width: 100%;
  min-height: 100%;
  background: #eeeeee;
  padding: 32rpx 30rpx 26rpx 30rpx;
  box-sizing: border-box;
}

.bill-list {
  width: 690rpx;
  height: 508rpx;
  background: #fff;
  border-radius: 8rpx;
  margin-bottom: 26rpx;
}

.bill-list:last-child {
  margin-bottom: 0;
}